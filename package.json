{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "server.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "dev": "concurrently \"nodemon server.js\" \"npm run client:dev\"",
    "client:dev": "cd client && npx vite",
    "client:build": "cd client && npx vite build",
    "client:preview": "cd client && npx vite preview",
    "server:dev": "nodemon server.js",
    "server:init_db": "npm run postinstall",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "node ./server/scripts/import-books.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cmatrix": "^0.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "framer-motion": "^10.16.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mongod": "^2.0.0",
    "mongoose": "^7.6.5",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "nodemon": "^2.0.22"
  }
}
